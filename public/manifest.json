{
  "manifest_version": 3,
  "name": "Color Extractor",
  "version": "0.1.0",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "src/popup/index.html",
    "default_icon": "icons/eye-34.png"
  },
  "devtools_page": "index.html",
  "icons": {
    "34": "icons/eye-34.png",
    "128": "icons/eye-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/index.js"]
    }
  ]
}
